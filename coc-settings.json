{
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "m",
    "function": ""
  },
  "snippets.priority": 98,
  "suggest.languageSourcePriority": 99,
  "coc.source.file.priority": 2,
  "coc.source.around.priority": 3,
  "coc.source.buffer.priority": 4,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "codeLens.enable": true,
  "java.referencesCodeLens.enabled": true,
  "java.implementationsCodeLens.enabled": true,
  "java.completion.enabled": true,
  "java.jdt.ls.vmargs": "-javaagent:/usr/local/share/lombok/lombok.jar",
  "eslint.autoFixOnSave": true,
  "eslint.autoFix": true,
  "list.indicator": ">",
  "list.selectedSignText": " ",
  "coc.preferences.hoverTarget": "float",
  "python.analysis.typeshedPaths": [],
  "python.venvPath": "./venv",
  "python.formatting.provider": "autopep8",
  "python.formatting.blackPath": "black",
  "python.formatting.blackArgs": [],
  "python.formatting.autopep8Path": "autopep8",
  "python.jediEnabled": false,
  "python.formatting.yapfPath": "yapf",
  "python.formatting.yapfArgs": [],
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--rcfile=/Users/kk/.config/nvim/utils/pylintrc"
  ],
  "python.linting.flake8Enabled": false,
  "python.linting.banditEnabled": false,
  "python.linting.mypyEnabled": false,
  "python.linting.prospectorEnabled": false,
  "python.linting.pydocstyleEnabled": false,
  "python.linting.pylamaEnabled": false,
  "snippets.ultisnips.directories": ["UltiSnips", "~/.config/nvim/snips"],
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips",
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "coc.preferences.rootPatterns": [".git", "package.json", "go.mod"],
  "suggest.noselect": false,
  "coc-playground.rootPath": "~/LocalDocuments/playground",
  "languageserver": {
    "lua": {
      "command": "/home/chris/.luambenvs/neovim4/bin/lua-lsp",
      "filetypes": ["lua"],
      "trace.server": "verbose"
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  },
  "bookmark.sign": "",
  "coc.source.emoji.filetypes": ["markdown"],
  "emmet.excludeLanguages": ["markdown", "vue", "html"],
  "explorer.width": 30,
  "explorer.file.root.template": "[icon] [git] [hidden & 1][root]",
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.icon.enableVimDevicons": false,
  "explorer.file.showHiddenFiles": false,
  "explorer.keyMappings.global": {
    "*": "toggleSelection", // 操 因为whichkey设置了很小的timeout时间，所以这里的keymap 两个char及以上的都需要按的很快才行
    "<2-LeftMouse>": [ "expandable?", ["expanded?", "collapse", "expand"], "open" ],
    "<<": "gitStage",
    "<bs>": ["wait", "gotoParent"],
    "<cr>": ["wait", "expandable?", "cd", "open"],
    "<esc>": "esc",
    "<tab>": "actionMenu",
    ">>": "gitUnstage",
    "?": "help",
    "A": "addDirectory",
    "D": "deleteForever",
    "E": "open:vsplit",
    "F": "searchRecursive",
    "II": "previewOnHover:disable",
    "Ic": "previewOnHover:toggle:content",
    "Il": "previewOnHover:toggle:labeling",
    "J": ["wait", "toggleSelection", "normal:j"],
    "K": ["wait", "toggleSelection", "normal:k"],
    "R": "refresh",
    "X": "systemExecute",
    "[D": ["wait", "markPrev:diagnosticError"],
    "[[": ["wait", "sourcePrev"],
    "[c": ["wait", "markPrev:git"],
    "[d": ["wait", "markPrev:diagnosticError:diagnosticWarning"],
    "[i": ["wait", "indentPrev"],
    "[m": ["wait", "markPrev:modified"],
    "]D": ["wait", "markNext:diagnosticError"],
    "]]": ["wait", "sourceNext"],
    "]c": ["wait", "markNext:git"],
    "]d": ["wait", "markNext:diagnosticError:diagnosticWarning"],
    "]i": ["wait", "indentNext"],
    "]m": ["wait", "markNext:modified"],
    "a": "addFile",
    "c": "cutFile",
    "d": "delete",
    "e": "open",
    "f": "search",
    ".": "toggleHidden",
    "gb": "gotoSource:buffer",
    "gd": "listDrive",
    "gf": "gotoSource:file",
    "gh": ["wait", "collapse:recursive"],
    "gj": ["wait", "expandableNext"],
    "gk": ["wait", "expandablePrev"],
    "gl": ["wait", "expand:recursive"],
    "gs": ["wait", "reveal:select"],
    "h": ["wait", "collapse"],
    "i": false, // cancel default mapkey
    "ic": "preview:content",
    "il": "preview:labeling",
    "l": ["wait", "expandable?", "expand", "open"],
    "o": ["wait", "expanded?", "collapse", "expand"],
    "p": "pasteFile",
    "q": "quit",
    "r": "rename",
    "s": "open:split",
    "t": "open:tab",
    "yn": "copyFilename",
    "yp": "copyFilepath",
    "yy": "copyFile",
    "zh": "toggleHidden"
  }
}
